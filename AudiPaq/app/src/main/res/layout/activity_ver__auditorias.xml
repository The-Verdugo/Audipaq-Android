<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity_ver_Auditorias">

    <TextView
        android:id="@+id/textView14"
        android:layout_width="414dp"
        android:layout_height="55dp"
        android:background="#19486E"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnBuscar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Buscar"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="33dp"
        tools:ignore="MissingConstraints" />

    <SearchView
        android:id="@+id/svBuscar"
        android:layout_width="252dp"
        android:layout_height="50dp"
        tools:layout_editor_absoluteX="143dp"
        tools:layout_editor_absoluteY="33dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="380dp"
        android:layout_height="40dp"
        android:text="Lista de actas de auditorias"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="117dp"
        tools:ignore="MissingConstraints"/>

    <TextView
        android:id="@+id/textView8"
        android:layout_width="398dp"
        android:layout_height="39dp"
        android:text="ID  FechaI  FechaF  Área  Dept  Aud  Status"
        android:textSize="20sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="190dp" />

    <ListView
        android:id="@+id/listviewAuditorias"
        android:layout_width="394dp"
        android:layout_height="440dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="235dp" />

</androidx.constraintlayout.widget.ConstraintLayout>